{
    "name": "Cidr Fermenter",
    "description": "Analyse cidr blocks",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_icon": "media/logo.png"
    },
    "permissions": [
        "contextMenus",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "background": {
        "service_worker": "scripts/background.js",
        "type": "module"
    }
}